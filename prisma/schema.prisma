generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Stock {
  id         Int      @id @default(autoincrement())
  name       String   @db.VarChar(255)
  count      Int      @default(0)
  url        String   @db.VarChar(500)
  created_at DateTime
  updated_at DateTime

  @@map("stock")
}
